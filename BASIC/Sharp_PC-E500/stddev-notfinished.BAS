100 DIM D(24)
110 INPUT "N=";N
120 PRINT "NUMBER=";N
130 C=1
140 REM LOOP
160 PRINT "DATA(";C;")=":INPUT D(C)
170 IF C<N THEN C=C+1:GOTO 140
180 N=C:PRINT "N OF DATA=";N
190 S=0
200 FOR I=1 TO N
210 S=S+D(I)
220 NEXT I
230 A=S/N
240 PRINT "AVR=";S/N
250 S=0
260 FOR I=1 TO N
270 R=D(I)-A: R=R*R
280 S=S+R
290 NEXT I
300 PRINT "S=";S
