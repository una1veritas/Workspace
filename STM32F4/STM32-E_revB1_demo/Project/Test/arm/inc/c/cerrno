#ifndef _CERRNO_
#define _CERRNO_

#ifndef _SYSTEM_BUILD
  #pragma system_include
#endif

#include <ycheck.h>
#include <yvals.h>

#ifdef _STD_USING
  #undef _STD_USING
  #include <errno.h>
  #define _STD_USING
#else /* _STD_USING */
  #include <errno.h>
#endif /* _STD_USING */

#ifdef _GLOBAL_USING
  _STD_BEGIN
  #ifndef errno
    #define errno errno
    using _CSTD errno;
  #endif
  _STD_END
#endif /* _GLOBAL_USING */
#endif /* _CERRNO_ */
