#ifndef _MAIN_H_
#define _MAIN_H_

#include <stdio.h>
#include <Arduino.h>

template<class T>
inline Print & operator <<(Print &out, T arg) {
  out.print(arg);
  return out;
}

const static char cr[] = "\r";
const static char nl[] = "\r\n";


// util funcs

size_t printHexString(Print &, uint8_t * , int );

#endif /* _MAIN_H_ */
