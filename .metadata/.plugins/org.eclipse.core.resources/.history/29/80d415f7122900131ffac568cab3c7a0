#include <stdio.h>
#include "main.h"
#include <Print.h>

size_t printByteHex(Print & prn, uint8_t b) {
	prn.print(b>>4 & 0x0f, HEX);
	prn.print(b & 0x0f, HEX);
	return 2;
}

size_t printHexString(Print & pr, uint8_t * a, int n) {
	size_t i;
    for(i = 0; i < n; i++) {
      pr.print(a[i]>>4, HEX);
      pr.print(a[i]& 0x0f, HEX);
      pr.print(' ');
    }
    return i*3;
}

size_t printTimestampString(Ptint & prn, uint32_t time, uint32_t date) {
	pr.print(time);
}
