#ifndef PRINTUTILITY_H_
#define PRINTUTILITY_H_

namespace util {

const char endl = '\n';
const char cr = '\r';
const char tab = '\t';

template<class T>
inline Stream &operator <<(Stream &stream, T arg) {
	arg.rintOn(stream);
	return stream;
}

class PrintUtility {

};

}

#endif /* PRINTUTILITY_H_ */
