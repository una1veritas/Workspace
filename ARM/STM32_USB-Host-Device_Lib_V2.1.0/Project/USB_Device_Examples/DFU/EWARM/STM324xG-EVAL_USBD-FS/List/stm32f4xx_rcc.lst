    917          
    918            /* Get PCLK2 prescaler */
    919            tmp = RCC->CFGR & RCC_CFGR_PPRE2;
    920            tmp = tmp >> 13;
    921            presc = APBAHBPrescTable[tmp];
    922            /* PCLK2 clock frequency */
