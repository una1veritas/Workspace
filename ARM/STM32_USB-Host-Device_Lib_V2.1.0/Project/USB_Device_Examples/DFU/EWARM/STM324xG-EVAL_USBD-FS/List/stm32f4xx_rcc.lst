    902            /* Compute HCLK, PCLK1 and PCLK2 clocks frequencies ------------------------*/
    903          
    904            /* Get HCLK prescaler */
    905            tmp = RCC->CFGR & RCC_CFGR_HPRE;
    906            tmp = tmp >> 4;
    907            presc = APBAHBPrescTable[tmp];
