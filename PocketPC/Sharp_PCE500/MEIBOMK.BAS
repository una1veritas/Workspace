180 REM SUB WRITE MEIBO.DAT
190 CLS
200 OPEN "F:MEIBO.DAT" FOR OUTPUT AS #1
210 RESTORE 1000
220 READ ID,CS$,NM$,GD$
230 PRINT #1,ID;",";CS$;",";NM$;",";GD$
240 IF ID=0 THEN GOTO 280
250 LOCATE 0,0
260 PRINT ID;" ";:PRINT USING "&&&&&&&&&&&&&&";NM$;:PRINT " ";GD$
270 GOTO 220
280 CLOSE #1
290 RETURN
300 REM SUB WRITE ATTEND.DAT
310 CLS
320 OPEN "F:ATTEND.DAT" FOR OUTPUT AS #1
330 RESTORE 1000
340 READ ID,CS$,NM$,GD$
350 PRINT #1,ID,"__________________"
360 IF ID=0 THEN GOTO 390
370 LOCATE 0,0: PRINT ID
380 GOTO 340
390 CLOSE #1
400 RETURN
