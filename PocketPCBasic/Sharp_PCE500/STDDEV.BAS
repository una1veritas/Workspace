100 REM STANDARD DEVIATION CALCULATOR
110 INPUT "ENTRY SIZE";N
120 DIM D(N)
130 FOR I=1 TO N : D(I)=0:NEXT I
140 FOR I=1 TO N
150 PRINT I;": ";:INPUT T
160 IF T < 0 GOTO 190
170 D(I)=T
180 NEXT I
190 C=I-1
200 PRINT "TOTAL ENTRY ";C
210 IF C=0 GOTO 290
220 S=D(1)
230 FOR I=2 TO C
240 S=S+D(I)
250 NEXT I
260 A=S/C
270 PRINT "SUM =";S;" AVR = ";A
290 FOR I=1 TO C
300 QS = QS + (D(I) - A)*(D(I) - A)
310 NEXT I
320 PRINT "POP. STDDEV = "; SQR (QS/C)
330 PRINT "SAM. STDDEV = "; SQR (QS/(C-1))
340 END
