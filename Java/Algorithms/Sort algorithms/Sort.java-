import java.lang.*;import java.util.Random;class Sort {	/*	static int lg(int i) {		return (int) Math.ceil(Math.log((double) i) / Math.log(2));	}*/	public static void bubbleSort(int a[]){		int i,j,tmp;		for(i=1;i < a.length;i++){			for(j=a.length-1 ; j>=i; j--){				if(a[j-1]>a[j]){					tmp=a[j];					a[j]=a[j-1];					a[j-1]=tmp;				}			}		}		return;	}	public static void mergeSort(int array[]) {		int buf[] = new int[array.length];		int i, w, s, e, cl, cr, ct;				for (w = 1; w < array.length; w = w * 2) {			//System.out.println("W: "+w);			for (s = 0; s < array.length; s = s + (2 * w)) {				e = s + (2*w);				//System.out.println(s+"-"+e);				cl = s; 				cr = s + w;				for (ct = s; ct < Math.min(s+(2*w), array.length); ct++) {					//System.out.println("cl: "+cl+ "  cr: "+cr+ "  ct: "+ct);					if (! (cr < array.length)) {						buf[ct] = array[cl];						cl++;						continue;					}					if (cl < s+w && cr < s+(2*w)) {						if (array[cl] < array[cr]) {							buf[ct] = array[cl];							cl++;						} else {							buf[ct] = array[cr];							cr++;						}					} else {						if (cl < s+w) {							buf[ct] = array[cl];							cl++;						} else {							buf[ct] = array[cr];							cr++;						}					}				}			}			for (i = 0; i < array.length; i++) {				array[i] = buf[i];			}			//System.out.println();		}		return;	}			public static void main(String args[]) {		int array[];		int size = 10;		Random rnd = new Random();		long ela;				if (args.length > 0) {			size = Integer.parseInt(args[0]);		}				array = new int[size];		for (int i = 0; i < size; i++) {			array[i] = (int) Math.abs(rnd.nextInt());		}				ela = System.currentTimeMillis();		Sort.mergeSort(array);		ela = System.currentTimeMillis() - ela;		System.out.println("Merge Sort: "+ (((float)ela)/1000));				array = new int[size];		for (int i = 0; i < size; i++) {			array[i] = (int) Math.abs(rnd.nextInt());		}				ela = System.currentTimeMillis();		Sort.bubbleSort(array);		ela = System.currentTimeMillis() - ela;		System.out.println("Bubble Sort: "+ (((float)ela)/1000));				for (int i = 0; i < size; i++) {			System.out.println(i + "\t" + array[i]);		}		return;	}}