#include <stdio.h>
#include <stdlib.h>

int main(int argc, char * argv[]) {
  int seed = 1;
  int n = 7;
  int range = 11;
  int i;

  printf("%d \n", argc);

  if (argc >= 1)
    n = atoi(argv[1]);
  if (argc >= 2) 
    seed = atoi(argv[2]);
  if (argc >= 3) {
    range = atoi(argv[3]);
    range = (range > RAND_MAX ? RAND_MAX : range);
  }
  srandom(seed);

  for (int i = 0; i < n; i++)
    printf("%d\n", random() % range);

  return 0;
}
